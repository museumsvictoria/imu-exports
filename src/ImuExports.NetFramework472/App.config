<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <appSettings>
    <add key="EmuServerHost" value="bunjil.mv.vic.gov.au" />
    <add key="EmuServerPort" value="40022" />
    <add key="serilog:minimum-level" value="Debug" />
    <add key="serilog:enrich:with-property:Environment" value="Development" />
    <add key="serilog:enrich:with-property:Application" value="Imu Exports" />
    <add key="AtlasOfLivingAustralia:SFTP:Host" value="host" />
    <add key="AtlasOfLivingAustralia:SFTP:Username" value="username" />
    <add key="AtlasOfLivingAustralia:SFTP:Password" value="password" />
    <add key="SeqUrl" value="http://godot.mv.vic.gov.au:5341" />
  </appSettings>
  <connectionStrings>
    <add name="LiteDB" connectionString="Filename=.\database.db" />
  </connectionStrings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Serilog" publicKeyToken="24c2f752a8e58a10" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.0.0.0" newVersion="2.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
    <gcAllowVeryLargeObjects enabled="true" />
  </runtime>
</configuration>